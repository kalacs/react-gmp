{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": false,
        "pure": true
      }
    ],
    [
      "transform-assets",
      {
        "extensions": [
          "png",
          "jpg"
        ],
        "name": "static/media/[name].[hash:8].[ext]"
      }
    ]    
  ]
}
